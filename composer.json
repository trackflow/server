{
    "name": "trackflow/server",
    "type": "project",
    "description": "PHP debugger application",
    "version": "v0.0.1",
    "license": "MIT",
    "require": {
        "php": "8.3.*",
        "ext-zlib": "*",
        "react/http": "^1.9",
        "react/socket": "^1.15",
        "rakibtg/sleekdb": "^2.15",
        "cboden/ratchet": "^0.4.4",
        "ratchet/pawl": "^0.4.1",
        "symfony/var-dumper": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.52"
    },
    "autoload": {
        "psr-4": {
            "App\\Debug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Debug\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
